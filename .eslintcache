,["31"],"import React, { useEffect, useState } from 'react';\r\nimport {BrowserRouter as Router, Switch, Route, Redirect, Link} from 'react-router-dom';\r\n\r\nimport User from './User';\r\nimport UserDetails from './UserDetail';\r\n\r\nimport './UsersList.css';\r\n\r\nconst API_URL = 'https://randomuser.me/api/';\r\nconst USERS_LIMIT = '10';\r\n\r\nfunction UsersList() {\r\n\r\n    const [users, setUsers] = useState([]);\r\n    const [isLoaded, setLoaded] = useState(false);\r\n    const [isError, setError] = useState(false);\r\n\r\n    const getUsers = () => {\r\n        fetch(`${API_URL}?results=${USERS_LIMIT}`)\r\n            .then(response => {\r\n                if(response.status == 200) {\r\n                    return response.json();\r\n                };\r\n                throw new Error('Wystąpił błąd');\r\n            })\r\n            .then(data => {\r\n                setUsers(data.results);\r\n                setLoaded(true);\r\n            })\r\n            .catch(error => {\r\n                console.log(error);\r\n                setLoaded(true);\r\n                setError(true);\r\n            });\r\n    }\r\n\r\n    useEffect(() => {\r\n        getUsers();\r\n    }, []);\r\n\r\n    const generateUsers = () => users.map((user, index) => {\r\n        const linkUrl = `/users/${index}`;\r\n        return (\r\n            <Link to={linkUrl} key={user.login.uuid}>\r\n                <User userData={user} />\r\n            </Link>\r\n        );\r\n    });\r\n\r\n    const getUserData = index => users[index];\r\n\r\n    const renderUsersList = () => {\r\n        if(isLoaded) {\r\n            return !isError ? generateUsers() : <h3>Wystąpił błąd. Spróbuj ponownie później.</h3>;\r\n        }\r\n        return <h3>Trwa ładowanie danych ...</h3>;\r\n    }\r\n\r\n    return (\r\n        <div className=\"page-wrapper\">\r\n            <Router>\r\n                <Switch>\r\n                    <Route path=\"/users/:id\">\r\n                        <div>\r\n                            <UserDetails getUserData={getUserData} />\r\n                        </div>\r\n                    </Route>\r\n                    <Route path=\"/users\">\r\n                        <div>\r\n                            {renderUsersList()}\r\n                        </div>\r\n                    </Route>\r\n                    <Route path=\"/\">\r\n                        <Redirect to=\"/users\" />\r\n                    </Route>\r\n                </Switch>\r\n            </Router>\r\n        </div>\r\n    );\r\n}\r\n\r\nexport default UsersList;\r\n","C:\\Users\\Karol\\Desktop\\Szkoła-Reacta\\users-list\\src\\components\\User.js",[],"C:\\Users\\Karol\\Desktop\\Szkoła-Reacta\\users-list\\src\\components\\UserDetail.js",["32"],"import React, { useEffect, useState } from 'react';\r\nimport {useParams, Link} from 'react-router-dom';\r\n\r\nimport User from './User';\r\nimport Map from './Map';\r\nimport './UserDetails.css';\r\n\r\nfunction UserDetails({getUserData}) {\r\n    const {id: index} = useParams();\r\n    const [userData, setUserData] = useState({});\r\n    const [isLoaded, setLoaded] = useState(false);\r\n\r\n    useEffect(() => {\r\n        const timer = setTimeout(() => {\r\n            setUserData(getUserData(index));\r\n            setLoaded(true);\r\n            clearTimeout(timer);\r\n        }, 1000);\r\n    }, []);\r\n\r\n    const renderUserDetails = () => {\r\n        if(isLoaded) {\r\n            return (\r\n                <div>\r\n                    <div style={{marginBottom: 24}}>\r\n                        <Link to=\"/users\">&lArr; Back to list</Link>\r\n                    </div>\r\n                    {userData ? <User userData={userData} /> : <h3>Brak danych</h3>}\r\n                </div>\r\n            );\r\n        }\r\n        return <h3>Trwa ładowanie danych ...</h3>;\r\n    }\r\n\r\n    return (\r\n        <>\r\n            <div className=\"user-details\">\r\n                {renderUserDetails()}\r\n            </div>\r\n            {(isLoaded && userData) && <Map coordinates={userData.location.coordinates} name={userData.name} />}\r\n        </>\r\n    );\r\n}\r\n\r\nexport default UserDetails;\r\n","C:\\Users\\Karol\\Desktop\\Szkoła-Reacta\\users-list\\src\\components\\Map.js",[],{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","severity":1,"message":"38","line":21,"column":36,"nodeType":"39","messageId":"40","endLine":21,"endColumn":38},{"ruleId":"41","severity":1,"message":"42","line":19,"column":8,"nodeType":"43","endLine":19,"endColumn":10,"suggestions":"44"},"no-native-reassign",["45"],"no-negated-in-lhs",["46"],"eqeqeq","Expected '===' and instead saw '=='.","BinaryExpression","unexpected","react-hooks/exhaustive-deps","React Hook useEffect has missing dependencies: 'getUserData' and 'index'. Either include them or remove the dependency array. If 'getUserData' changes too often, find the parent component that defines it and wrap that definition in useCallback.","ArrayExpression",["47"],"no-global-assign","no-unsafe-negation",{"desc":"48","fix":"49"},"Update the dependencies array to be: [getUserData, index]",{"range":"50","text":"51"},[574,576],"[getUserData, index]"]